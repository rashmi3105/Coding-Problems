#include<bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
     int mat[n][n];
     for(int i=0;i<n;i++)
     { 
         for(int j=0;j<n;j++)
         {
             mat[i][j]= 0;
         }
    }
      for(int i=0;i<n;i++)
     { 
         
             mat[i][0]= 1;
     }
     for(int i=1;i<n;i++)
     { 
         for(int j=1;j<n;j++)
         {
             mat[i][j] = mat[i-1][j] + mat[i-1][j-1];
         }
    }
	// Write your code here
     for(int i=0;i<n;i++)
     { 
         for(int j=0;j<n;j++)
         {
             if(mat[i][j]!=0)
             {
                 cout<<mat[i][j]<<" ";
             }
         }
         cout<<endl;
    }
    return 0;
}
